{
  "name": "not-a-real-shop",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "dev": "nx run-many --target=serve --parallel=10 --all=true",
    "postinstall": "prisma generate",
    "prisma:generate": "prisma generate",
    "prisma:migrate:dev": "prisma migrate dev",
    "prisma:studio": "prisma studio",
    "proto:users": "protoc -I=proto --plugin=node_modules/.bin/protoc-gen-ts_proto --ts_proto_out=libs/rpc/src/lib proto/users.proto --ts_proto_opt=addGrpcMetadata=true --ts_proto_opt=nestJs=true --ts_proto_opt=fileSuffix=.pb",
    "proto:orders": "protoc -I=proto --plugin=node_modules/.bin/protoc-gen-ts_proto --ts_proto_out=libs/rpc/src/lib proto/orders.proto --ts_proto_opt=addGrpcMetadata=true --ts_proto_opt=nestJs=true --ts_proto_opt=fileSuffix=.pb",
    "proto:products": "protoc -I=proto --plugin=node_modules/.bin/protoc-gen-ts_proto --ts_proto_out=libs/rpc/src/lib proto/products.proto --ts_proto_opt=addGrpcMetadata=true --ts_proto_opt=nestJs=true --ts_proto_opt=fileSuffix=.pb",
    "proto:all": "npm run proto:users && npm run proto:orders && npm run proto:products",
    "test": "nx affected -t test --parallel=3"
  },
  "private": true,
  "dependencies": {
    "@grpc/grpc-js": "^1.8.14",
    "@grpc/proto-loader": "^0.7.7",
    "@nestjs/common": "^9.1.1",
    "@nestjs/core": "^9.1.1",
    "@nestjs/microservices": "^9.4.1",
    "@nestjs/platform-express": "^9.1.1",
    "@prisma/client": "^4.14.1",
    "@swc/helpers": "~0.5.0",
    "axios": "^1.0.0",
    "nestjs-zod": "^2.3.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-router-dom": "6.8.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.0",
    "tslib": "^2.3.0",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@golevelup/ts-jest": "^0.3.7",
    "@nestjs/schematics": "^9.1.0",
    "@nestjs/swagger": "^6.3.0",
    "@nestjs/testing": "^9.1.1",
    "@nx/cypress": "^16.2.1",
    "@nx/eslint-plugin": "16.2.1",
    "@nx/jest": "16.2.1",
    "@nx/js": "16.2.1",
    "@nx/linter": "16.2.1",
    "@nx/nest": "^16.2.1",
